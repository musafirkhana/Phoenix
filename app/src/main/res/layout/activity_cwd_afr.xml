<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_home">
    <RelativeLayout
        android:id="@+id/top_linear"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        android:background="@color/black">

        <RelativeLayout
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
           >

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:clickable="true"
                android:onClick="BACK"
                android:layout_centerInParent="true"
                android:src="@drawable/ic_arrow_left">

            </ImageView>


        </RelativeLayout>

        <TextView
            android:id="@+id/topbar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="CWD PANEL"
            android:fontFamily="@font/my_custom_font"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:textStyle="bold|normal" />

    </RelativeLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/top_linear"
        android:background="@color/background_home">
    <LinearLayout
        android:id="@+id/layout"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_margin="10sp"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="5dp"
            android:layout_height="60dp">
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:onClick="AFR1"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="FUEL\n CROSSFEED"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:onClick="AFR2"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="AUT. BANK\n CONTROL"
                    android:layout_centerInParent="true"
                    android:textColor="@color/cwd_green"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:onClick="AFR3"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="AUT. BANK\n CONTROL"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="5dp"
            android:layout_height="60dp">
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:onClick="AFR4"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="SPOILERS"
                    android:gravity="center"
                    android:layout_centerInParent="true"
                    android:textColor="@color/cwd_green"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:onClick="AFR5"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="FUEL \nTRANSFER"
                    android:layout_centerInParent="true"
                    android:textColor="@color/cwd_green"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:onClick="AFR6"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="STALLING"
                    android:textStyle="bold"
                    android:layout_centerInParent="true"
                    android:textColor="@color/red"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="5dp"
            android:layout_height="60dp">
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:onClick="AFR7"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="DOOR"
                    android:layout_centerInParent="true"
                    android:textColor="@color/red"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:onClick="AFR8"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="ACTUATE LH\n TRANSFER"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:onClick="AFR9"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="ACTUATE RH\n TRANSFER"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="5dp"
            android:layout_height="60dp">
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:onClick="AFR10"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="SEARCH\nLIGHT"
                    android:layout_centerInParent="true"
                    android:textColor="@color/cwd_green"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:onClick="AFR11"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="EXTEND LAND\n GEAR"
                    android:layout_centerInParent="true"
                    android:textColor="@color/cwd_green"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:onClick="AFR12"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="ENGINE\n STARTING"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="5dp"
            android:layout_height="60dp">
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:onClick="AFR13"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="EXTEND LAND\n GEAR"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:onClick="AFR14"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="BAG. COMP. FIRE"
                    android:layout_centerInParent="true"
                    android:textColor="@color/red"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:onClick="AFR15"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="CABIN CALL"
                    android:layout_centerInParent="true"
                    android:textColor="@color/google_white"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>


        </LinearLayout>



    </LinearLayout>
    </ScrollView>
</RelativeLayout>
