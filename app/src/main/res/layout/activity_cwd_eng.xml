<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_home">
    <RelativeLayout
        android:id="@+id/top_linear"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        android:background="@color/black">

        <RelativeLayout
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
           >

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:clickable="true"
                android:onClick="BACK"
                android:layout_centerInParent="true"
                android:src="@drawable/back_selector">

            </ImageView>


        </RelativeLayout>

        <TextView
            android:id="@+id/topbar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="CWD PANEL"
            android:fontFamily="@font/my_custom_font"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:textStyle="bold|normal" />

    </RelativeLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/top_linear"
        android:background="@color/background_home">
    <LinearLayout
        android:id="@+id/layout"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_margin="10sp"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="5dp"
            android:layout_height="60dp">
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:onClick="ENG1"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="FIRE"
                    android:layout_centerInParent="true"
                    android:textColor="@color/red"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:onClick="ENG2"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="OIL         PRESSURE"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:onClick="ENG3"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="FUEL PRESSURE"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="5dp"
            android:layout_height="60dp">
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="MINIMUM     FUEL"
                    android:gravity="center"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="ENG. OVER   LIMIT"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="CHIPS"
                    android:textStyle="bold"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="5dp"
            android:layout_height="60dp">
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="ISOLATION VALVE"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="FUEL       BY-PASS"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text=" BETA  RANGE "
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="5dp"
            android:layout_height="60dp">
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="LIMITER   ACTIVE"
                    android:layout_centerInParent="true"
                    android:textColor="@color/cwd_green"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="AUTO     FEATHER"
                    android:layout_centerInParent="true"
                    android:textColor="@color/cwd_green"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="FEATHER    PUMP"
                    android:layout_centerInParent="true"
                    android:textColor="@color/google_white"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="5dp"
            android:layout_height="60dp">
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="HYDRAUL."
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="OIL BY-PASS"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>



        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="5dp"
            android:layout_height="60dp">
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="PROPELLER  DEICING"
                    android:layout_centerInParent="true"
                    android:textColor="@color/amber"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_weight="2"
                android:layout_marginLeft="5dp"
                android:background="@drawable/rectangle_cwd_layout"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:text="SEPARATOR      VANE"
                    android:layout_centerInParent="true"
                    android:textColor="@color/cwd_green"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:layout_height="wrap_content">

                </TextView>

            </RelativeLayout>



        </LinearLayout>



    </LinearLayout>
    </ScrollView>
</RelativeLayout>
